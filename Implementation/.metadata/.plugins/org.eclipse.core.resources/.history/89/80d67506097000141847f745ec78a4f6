package Modele;

import java.util.*;

import org.w3c.dom.*;

/**
 * 
 */
public class Plan {

    /**
     * 
     */
    public Plan() {
    }
    
    public Boolean initPlan(Element xmlChunk)
    {
    	NodeList listeNoeuds = xmlChunk.getElementsByTagName("Noeud");
    	for (int i = 0; i<listeNoeuds.getLength(); i++)
    	{
    		Noeud noeudActuel = new Noeud();
    		noeudActuel.initNoeud(listeNoeuds.item(i));
    		this.listeNoeuds.add(noeudActuel);
    	}
		return null;
    }
    
    private List<Noeud> listeNoeuds;
    
    private List<Troncon> listeTroncons;

}